  <metadata>
    <id>explorergenie</id>
    <version>2.0.1</version>
    <owners>elpatron</owners>
    <title>ExplorerGenie (Install)</title>
    <authors>Martin Stoeckli</authors>
    <projectUrl>https://www.martinstoeckli.ch/explorergenie/</projectUrl>
    <iconUrl>https://www.martinstoeckli.ch/explorergenie/icon32.png</iconUrl>
